<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.omar.C196">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.omar.C196.HomeScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.omar.C196.CourseScreen"
            android:parentActivityName="com.example.omar.C196.HomeScreen"></activity>
        <activity android:name="com.example.omar.C196.CourseDetailScreen"
            android:parentActivityName="com.example.omar.C196.CourseScreen"></activity>
        <activity android:name="com.example.omar.C196.NewTermScreen"
            android:parentActivityName="com.example.omar.C196.TermScreen"></activity>
        <activity android:name="com.example.omar.C196.TermScreen"
            android:parentActivityName="com.example.omar.C196.HomeScreen"></activity>
        <activity android:name="com.example.omar.C196.TermDetailScreen"
            android:parentActivityName="com.example.omar.C196.TermScreen"></activity>
        <activity android:name="com.example.omar.C196.AssessmentScreen"
            android:parentActivityName="com.example.omar.C196.CourseScreen"></activity>
        <activity android:name="com.example.omar.C196.NewAssessmentScreen"
            android:parentActivityName="com.example.omar.C196.AssessmentScreen"></activity>
        <activity android:name="com.example.omar.C196.NewCourseScreen"
            android:parentActivityName="com.example.omar.C196.CourseScreen"></activity>
        <activity android:name="com.example.omar.C196.CourseEditScreen"></activity>
        <activity android:name="com.example.omar.C196.AssessmentDetailScreen"></activity>
        <activity android:name="com.example.omar.C196.AssessmentEditScreen"></activity>
        <activity android:name="com.example.omar.C196.TermEditScreen"></activity>
        <activity android:name=".MentorScreen"
            android:parentActivityName=".HomeScreen"></activity>
        <activity android:name=".MentorDetailScreen"
            android:parentActivityName=".MentorScreen"></activity>
        <activity android:name=".MentorEditScreen"
            android:parentActivityName=".MentorDetailScreen"></activity>
        <activity android:name=".NewMentorScreen"
            android:parentActivityName=".MentorScreen"></activity>
    </application>

</manifest>